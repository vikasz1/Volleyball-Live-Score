<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vikas.realtimedatabase">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/volleylogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/volleylogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.RealtimeDataBase">
        <activity
            android:name=".Public_Score"
            android:exported="true" />
        <activity
            android:name=".Event_Display"
            android:exported="true" />
        <activity
            android:name=".Admin_Score"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".splashscreen"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>